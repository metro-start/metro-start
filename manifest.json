{
	"name": "Metro Start",
	"version": "10.0",
	"manifest_version": 2,
	"description": "A new tab page for Google Chrome inspired by Zune",
	"chrome_url_overrides": {
		"newtab": "start.html"
	},
	"icons": {
		"16": "icons/icon16.png",
		"32": "icons/icon32.png",
		"128": "icons/icon128.png"
	},
	"web_accessible_resources": [
  		"bundle.map"
	],
	"permissions": [
		"http://*.appspot.com/*",
		"http://query.yahooapis.com/*",
		"management",
		"bookmarks",
		"storage",
		"sessions",
		"tabs"
	]
}
